# Network configuration
VITE_NETWORK_ID=Undeployed

# Logging level
VITE_LOGGING_LEVEL=trace

# API deployment mode
# Options: 'real', 'mock', or leave undefined for manual deploy buttons
# VITE_API_FORCE_DEPLOY=mock

# Join an existing contract instead of deploying
# Set this to the contract address from `yarn deploy` output
# When set, the app will join this contract instead of deploying a new one
# VITE_CONTRACT_ADDRESS=<contract-address-here>

# Batcher mode configuration (optional)
VITE_BATCHER_MODE_ENABLED=false
# VITE_BATCHER_MODE_INDEXER_HTTP_URL=http://127.0.0.1:8088/api/v1/graphql
# VITE_BATCHER_MODE_INDEXER_WS_URL=ws://127.0.0.1:8088/api/v1/graphql/ws
# VITE_BATCHER_MODE_BATCHER_URL=http://localhost:8000
